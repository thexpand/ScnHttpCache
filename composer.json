{
    "name": "thexpand/zf2-http-cache",
    "description": "Adds several features to help make ZF2 applications cacheable.",
    "type": "library",
    "keywords": [
        "zf2",
        "cache-control",
        "esi",
        "edge side includes",
        "varnish"
    ],
    "homepage": "https://github.com/thexpand/zf2-http-cache",
    "authors": [
        {
            "name": "Tsvetomir Lazarov",
            "email": "cvetomir.lazarov@gmail.com",
            "homepage": "https://github.com/thexpand"
        }
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "2.*"
    },
    "require-dev": {
        "mockery/mockery": ">=0.7.2"
    },
    "autoload": {
        "psr-4": {
            "Thexpand\\": "src/"
        }
    },
    "scripts": {
        "generate-classmap": "./vendor/bin/classmap_generator.php --library src --output ./autoload_classmap.php --overwrite"
    }
}
